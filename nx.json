{
  "npmScope": "hydrocube",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "functions": {
      "tags": []
    },
    "console": {
      "tags": []
    },
    "console-e2e": {
      "tags": [],
      "implicitDependencies": ["console"]
    },
    "mobile": {
      "tags": []
    },
    "device": {
      "tags": []
    }
  }
}
